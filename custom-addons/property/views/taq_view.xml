<odoo>
    <record  id="taq_view_tree" model="ir.ui.view">
		<field name="name">Taq Tree</field>
		<field name="model">taq</field>
		<field name="arch" type="xml">
			<tree multi_edit="1">
				<field name="name"/>
			</tree>
		</field>
	</record>

	<record  id="taq_view_form" model="ir.ui.view">
		<field name="name">Taq Form</field>
		<field name="model">taq</field>
		<field name="arch" type="xml">
			<form>
				<sheet>
					<div class="oe_title">
						<label for="name"/>
						<h1>
							<field name="name"/>
						</h1>
					</div>
					<group>
						<group>
							<field name="property_ids" widget="many2many_tags"/>
						</group>
					</group>
				</sheet>
			</form>
		</field>
	</record>

	<record  id="taq_action" model="ir.actions.act_window">
		<field name="name">Taq</field>
		<field name="res_model">taq</field>
	</record>

	<menuitem id="taq_menu_item"
	name="Taqs"
	parent="configration_menu"
	action="taq_action"
	/>
</odoo>